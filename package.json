{
  "name": "apto-ui",
  "description": "Apto UI for Angular",
  "homepage": "https://github.com/aptotude/apto-ui",
  "bugs": "https://github.com/aptotude/apto-ui/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/aptotude/apto-ui.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "ng": "ng",
    "gulp": "gulp",
    "dev": "yarn storybook:local",
    "build": "ng build apto-ui-lib && gulp package",
    "icons": "gulp sprites",
    "lint": "ng lint",
    "npm:pack": "cd dist/apto-ui-lib && npm pack",
    "package": "yarn build && yarn npm:pack",
    "test": "ng test apto-ui-lib",
    "test:headless": "ng test apto-ui-lib --watch=false --browsers=ChromeHeadless",
    "release": "./scripts/release.sh",
    "storybook:local": "start-storybook -p 9001 -s ./apto-ui-lib/src/assets -c .storybook",
    "storybook:deploy": "storybook-to-ghpages"
  },
  "license": "MIT",
  "version": "1.0.81",
  "engines": {
    "node": ">=10.11",
    "yarn": ">=1.3.2"
  },
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "intl": "~1.2.5",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/cli": "^6.2.8",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.0",
    "@storybook/addon-actions": "^3",
    "@storybook/addon-knobs": "^3",
    "@storybook/addon-notes": "^3",
    "@storybook/addon-options": "^3",
    "@storybook/addons": "^3",
    "@storybook/angular": "^3",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/jasmine": "3.3.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "babel-core": "^6.26.3",
    "codelyzer": "~4.3.0",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-replace": "^1.0.0",
    "gulp-svg-sprites": "^4.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.1.0",
    "node-sass": "~4.9.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "run-sequence": "^2.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "2.9.2"
  }
}

{
  "name": "apto-ui",
  "description": "Apto UI for Angular",
  "homepage": "https://github.com/aptotude/apto-ui",
  "bugs": "https://github.com/aptotude/apto-ui/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/aptotude/apto-ui.git"
  },
  "scripts": {
    "ng": "ng",
    "gulp": "gulp",
    "dev": "yarn storybook:local",
    "release": "./scripts/release.sh",
    "test": "ng test --browsers Chrome",
    "test:headless": "ng test --browsers ChromeHeadless --single-run --progress=false",
    "lint": "ng lint",
    "icons": "gulp sprites",
    "storybook:local": "start-storybook -p 9001 -s ./src/assets -c .storybook",
    "storybook:deploy": "storybook-to-ghpages",
    "packagr": "ng-packagr -p src/lib/package.json && gulp publish -o docs"
  },
  "license": "MIT",
  "version": "1.0.45",
  "engines": {
    "node": ">= 8.9.0"
  },
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@storybook/addon-knobs": "^3.4.10",
    "core-js": "^2.5.3",
    "rxjs": "^5.5.7",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/language-service": "^5.2.9",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-notes": "^3.4.8",
    "@storybook/addon-options": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/angular": "^3.4.8",
    "@storybook/storybook-deployer": "^2.3.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "babel-core": "^6.26.3",
    "codelyzer": "^4.0.1",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-replace": "^1.0.0",
    "gulp-svg-sprites": "^4.1.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.6",
    "protractor": "~5.1.2",
    "run-sequence": "^2.2.1",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.4.2"
  }
}
